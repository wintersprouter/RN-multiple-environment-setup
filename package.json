{
  "name": "project",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android:staging": "react-native run-android --variant=stagingdebug",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:dev": "npx react-native run-android --variant=developmentdebug --appIdSuffix development",
    "android:dev-release": "npx react-native run-android --variant=developmentrelease --appIdSuffix development",
    "android:dev-release-aab": "cd android && ./gradlew bundleDevelopmentRelease",
    "android:dev-release-apk": "cd android && ./gradlew assembleDevelopmentRelease",
    "android:prod": "npx react-native run-android --variant=productiondebug",
    "android:prod-release": "npx react-native run-android --variant=productionrelease",
    "android:prod-release-aab": "cd android && ./gradlew bundleProductionRelease",
    "android:prod-release-apk": "cd android && ./gradlew assembleProductionRelease"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-config": "^1.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
